{
  "name": "@tboyt/react-flip-toolkit",
  "version": "6.4.1",
  "description": "Configurable FLIP animation helpers for React",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "browser": "umd/react-flip-toolkit.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/aholachek/react-flip-toolkit"
  },
  "bugs": "https://github.com/aholachek/react-flip-toolkit/issues",
  "author": "Alex Holachek",
  "keywords": [
    "react-component",
    "FLIP",
    "transition",
    "animation"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "demo": "parcel --no-cache demo/src/index.html",
    "deploy-demo": "parcel build demo/src/index.html --out-dir demo/dist; cp demo/dist/index.html demo/dist/200.html; surge demo/dist --domain react-flip-toolkit-demos.surge.sh",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "check-types": "tsc",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "inspect": "npx source-map-explorer dist/index.umd.js dist/index.umd.js.map",
    "lint": "tslint src",
    "tslint-fix": "tslint --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}",
    "format-and-fix": "npm-run-all format tslint-fix",
    "test:dom": "parcel mocha/testrunner.html",
    "test": "jest",
    "test:all": "npm-run-all test test:dom",
    "test:core": "parcel 'src/__tests__/core-test/index.html'",
    "preversion": "npm test",
    "version": "npm run build"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "rematrix": "0.2.2"
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-transform-typescript": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "7.1.0",
    "@babel/runtime": "^7.2.0",
    "@types/jest": "^23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "animejs": "^2.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-react-app": "^7.0.0",
    "chai": "^4.2.0",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.11.0",
    "prettier": "1.15.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "^2.1.2",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "rollup": "^1.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^4.0.1",
    "rollup-plugin-typescript2": "^0.18.1",
    "sinon": "^7.2.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.2"
  },
  "files": [
    "es",
    "lib",
    "umd",
    "types"
  ]
}
